{
    "name": "mongaku",
    "version": "0.0.1",
    "description": "An image similarity search engine and database.",
    "main": "build/mongaku.js",
    "bin": {
        "mongaku": "build/bin/mongaku.js"
    },
    "scripts": {
        "build": "./src/bin/mongaku.js build",
        "build-watch": "./src/bin/mongaku.js build-watch",
        "client:prod": "NODE_ENV=production webpack -p",
        "lint": "eslint src && npm run flow",
        "postinstall": "npm run build",
        "pretest": "cp -R src/tests/data/ build/tests/data/",
        "test": "npm run lint && NODE_ENV=test tap --cov build/tests/*/*.js",
        "start": "./build/bin/mongaku.js start",
        "stop": "./build/bin/mongaku.js stop",
        "restart": "./build/bin/mongaku.js restart",
        "dev": "./build/bin/mongaku.js dev",
        "dev-test": "NODE_ENV=test supervisor -e js,jsx -w build -i node_modules,locales build/tests/server.js",
        "extract-i18n-js": "babel-jsxgettext build/*.js build/*/*.js build/*/*/*.js build/*/*/*/*.js translations/messages.pot",
        "extract-i18n": "npm run extract-i18n-js && git add translations/messages.pot",
        "update-i18n": "po2json.js -p translations/de/messages.po > translations/de/messages.json && po2json.js -p translations/it/messages.po > translations/it/messages.json && po2json.js -p translations/ja/messages.po > translations/ja/messages.json",
        "migrate": "migrate",
        "coverage": "NODE_ENV=test tap --cov --coverage-report=lcov build/tests/*/*.js",
        "test-file": "NODE_ENV=test tap",
        "flow": "flow",
        "prettier": "prettier --write \"src/**/*.js\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mongaku/mongaku.git"
    },
    "author": "John Resig <jeresig@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mongaku/mongaku/issues"
    },
    "homepage": "https://github.com/mongaku/mongaku",
    "dependencies": {
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "async": "2.6.1",
        "bcryptjs": "2.4.3",
        "body-parser": "1.18.3",
        "concat-stream": "1.6.2",
        "connect-mongo": "2.0.1",
        "cookie-parser": "1.4.3",
        "core-js": "^3.6.5",
        "csurf": "1.9.0",
        "csv-streamify": "4.0.0",
        "dotenv": "6.0.0",
        "elasticsearch-sanitize": "2.0.0",
        "express": "4.16.3",
        "express-session": "1.15.6",
        "farmhash": "2.1.0",
        "formidable": "1.2.1",
        "glob": "7.1.3",
        "gm": "1.23.1",
        "iconv-lite": "0.4.24",
        "imageinfo": "1.0.4",
        "intl": "1.2.5",
        "jsondiffpatch": "0.3.11",
        "jsonlint": "1.6.3",
        "kerberos": "0.0.23",
        "libxmljs": "0.19.7",
        "lodash.clonedeepwith": "4.5.0",
        "method-override": "3.0.0",
        "minimist": "1.2.0",
        "mongoosastic": "4.4.1",
        "mongoose": "5.2.12",
        "mongoose-version": "1.1.0",
        "morgan": "1.9.0",
        "naught": "1.6.0",
        "parse-dimensions": "0.0.8",
        "passport": "0.4.0",
        "passport-local": "1.0.0",
        "password-generator": "2.2.0",
        "pastec": "0.0.5",
        "prop-types": "15.6.2",
        "react": "16.4.2",
        "react-dom": "16.4.2",
        "readline-sync": "1.4.9",
        "request": "2.88.0",
        "serve-favicon": "2.5.0",
        "serve-static": "1.13.2",
        "shelljs": "0.8.5",
        "unzip-stream": "0.3.0",
        "valid-url": "1.0.9",
        "webpack-cli": "^3.1.0",
        "whatwg-fetch": "2.0.4",
        "yearrange": "0.0.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-flow": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "9.0.0",
        "babel-jsxgettext": "0.5.1",
        "babel-loader": "^8.0.0",
        "babel-plugin-flow-react-proptypes": "24.1.1",
        "babel-plugin-module-resolver": "3.1.1",
        "eslint": "5.5.0",
        "eslint-config-prettier": "3.0.1",
        "eslint-plugin-flowtype": "2.50.0",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "7.11.1",
        "exports-loader": "0.7.0",
        "flow-bin": "0.80.0",
        "migrate": "1.6.1",
        "mock-fs": "4.6.0",
        "null-loader": "0.1.1",
        "pre-commit": "1.2.2",
        "prettier": "1.14.2",
        "sinon": "6.1.5",
        "supervisor": "0.12.0",
        "tap": "12.0.1",
        "webpack": "4.17.1"
    },
    "pre-commit": [
        "build",
        "lint",
        "extract-i18n"
    ],
    "nyc": {
        "exclude": [
            "src/tests",
            "src/server/cron.js",
            "src/converters"
        ],
        "extension": [
            ".jsx"
        ]
    }
}
